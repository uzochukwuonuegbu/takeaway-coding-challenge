{
  "name": "takeaway-coding-challenge",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "init-schema": "ts-node initSchema.ts",
    "test": "jest --coverage --config ./test/jest.config.json  --runInBand --detectOpenHandles --forceExit",
    "test:integration": "jest --config ./test/jest.config.integration.json --runInBand --forceExit",
    "start": "npm run copy:env:prod && DOTENV_CONFIG_PATH=.env npm run init-schema && npm run build && DOTENV_CONFIG_PATH=.env node -r dotenv/config ./.build/src/main.js",
    "dev": "npm run copy:env:dev && DOTENV_CONFIG_PATH=dev.env npm run init-schema && DOTENV_CONFIG_PATH=dev.env ts-node -r dotenv/config ./src/main.ts",
    "build": "npm run lint && npm run clean && tsc",
    "lint": "tslint --project .",
    "clean": "rimraf ./.build",
    "copy:env:dev": "cp dev.env.sample dev.env",
    "copy:env:prod": "cp env.sample .env"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-serve-static-core": "^0.1.1",
    "husky": "^8.0.3",
    "joi": "^17.9.1",
    "jsonwebtoken": "^9.0.1",
    "pg": "^8.11.1",
    "sequelize": "^6.31.0",
    "swagger-ui-express": "^4.6.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/jest": "^24.0.25",
    "@types/node": "^20.3.2",
    "@types/pg": "^8.10.2",
    "jest": "^29.4.3",
    "prettier": "^1.19.1",
    "rimraf": "^4.1.2",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tslint": "^5.20.1",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "5.1.6"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
